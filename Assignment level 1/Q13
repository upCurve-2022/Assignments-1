package com.company;

import java.util.Scanner;

public class Series1 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int y=1;
        for(int i=1;i<=n;i++)
        {
             for(int j=1;j<=i;j++)
             {
                 y=y*i;
             }
             System.out.print(y+" ");
             y=1;
        }
    }
}


output

5
1 4 27 256 3125 
