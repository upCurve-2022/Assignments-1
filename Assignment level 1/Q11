package com.company;

import java.util.Scanner;

public class Series1 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=2;i<=n;i++)
        {
             if(i%2==0)
             {
                 System.out.print(i*i+" ");
             }
        }
    }
}

output

10
4 16 36 64 100 
